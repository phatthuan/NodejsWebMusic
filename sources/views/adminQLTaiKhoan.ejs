<!DOCTYPE html>
<html lang="en">

<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>ADMIN - Zing MP3</title>
    <meta content='width=device-width, initial-scale=1.0, shrink-to-fit=no' name='viewport' />
    <link rel="icon" href="/images/icon-home/small-logo.svg" type="image/gif" sizes="16x16" />

    <!-- Fonts and icons -->
    <script src="/admin/js/plugin/webfont/webfont.min.js"></script>
    <script src="/admin/js/admin1.js"></script>

    <!-- CSS Files -->
    <link rel="stylesheet" href="/admin/css/bootstrap.min.css">
    <link rel="stylesheet" href="/admin/css/atlantis.min.css">

</head>

<body>
    <div class="wrapper">

        <!-- Header -->
        <div class="main-header">
            <!-- Logo Header -->
            <div class="logo-header" data-background-color="blue">

                <a href="#" class="logo">
                    <img style="width: 60%;" src="/images/sidebar-icon/logo/logo-dark.svg" alt="navbar brand"
                        class="navbar-brand">
                </a>

                <button class="navbar-toggler sidenav-toggler ml-auto" type="button" data-toggle="collapse"
                    data-target="collapse" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon">
                        <i class="icon-menu"></i>
                    </span>
                </button>

                <button class="topbar-toggler more"><i class="icon-options-vertical"></i></button>
                <div class="nav-toggle">
                    <button class="btn btn-toggle toggle-sidebar">
                        <i class="icon-menu"></i>
                    </button>
                </div>
            </div>
            <!-- End Logo Header -->

            <!-- Navbar Header -->
            <nav class="navbar navbar-header navbar-expand-lg" data-background-color="blue2">

                <div class="container-fluid">

                    <!-- Search -->
                    <div class="collapse" id="search-nav">
                        <form class="navbar-left navbar-form nav-search mr-md-3">
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <button type="submit" class="btn btn-search pr-1">
                                        <i class="fa fa-search search-icon"></i>
                                    </button>
                                </div>
                                <input type="text" placeholder="Tìm kiếm ..." class="form-control">
                            </div>
                        </form>
                    </div>

                    <ul class="navbar-nav topbar-nav ml-md-auto align-items-center">

                        <li class="nav-item dropdown hidden-caret">
                            <a class="dropdown-toggle profile-pic" data-toggle="dropdown" href="#"
                                aria-expanded="false">
                                <div class="avatar-sm">
                                    <img src="data:image/png;base64," alt="..." class="avatar-img rounded-circle">
                                </div>
                            </a>
                            <ul class="dropdown-menu dropdown-user animated fadeIn">
                                <div class="dropdown-user-scroll scrollbar-outer">
                                    <li>
                                        <div class="user-box">
                                            <div class="avatar-lg"><img src="data:image/png;base64," alt="image profile"
                                                    class="avatar-img rounded"></div>
                                            <div class="u-text">
                                                <h4 class="name_user"></h4>
                                                <p class="email_user text-muted"></p>
                                            </div>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="dropdown-divider"></div>
                                        <a id="logout_button" class="dropdown-item"><i class="fas fa-sign-out-alt"></i> Logout</a>
                                    </li>
                                </div>
                            </ul>
                        </li>

                    </ul>
                </div>

            </nav>
            <!-- End Navbar -->
        </div>
        <!-- End Header -->

        <!-- Sidebar -->
        <div class="sidebar sidebar-style-2">
            <div class="sidebar-wrapper scrollbar scrollbar-inner">
                <div class="sidebar-content">
                    <!-- sidebar user -->
                    <div class="user">
                        <div class="avatar-sm float-left mr-2">
                            <img src="data:image/png;base64," alt="..." class="avatar-img rounded-circle">
                        </div>
                        <div class="info">
                            <a>
                                <span class="name_sidebar">


                                </span>
                            </a>
                            <div class="clearfix"></div>
                        </div>
                    </div>

                    <ul class="nav nav-primary">
                        <li class="nav-section">
                            <span class="sidebar-mini-icon">
                                <i class="fa fa-ellipsis-h"></i>
                            </span>
                            <h4 class="text-section">Manager</h4>
                        </li>

                        <li class="nav-item active">
                            <a data-toggle="collapse" href="#usermanager">
                                <i class="fas fa-regular fa-users"></i>
                                <p>Quản lý người dùng</p>
                                <span class="caret"></span>
                            </a>
                            <div class="collapse" id="usermanager">
                                <ul class="nav nav-collapse">
                                    <li>
                                        <a href="/admin/account">
                                            <span class="sub-item">Tài khoản</span>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="/admin/comment">
                                            <span class="sub-item">Bình luận</span>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </li>

                        <li class="nav-item">
                            <a data-toggle="collapse" href="#websitemanager">
                                <i class="fas fa-th-list"></i>
                                <p>Quản lý website</p>
                                <span class="caret"></span>
                            </a>
                            <div class="collapse" id="websitemanager">
                                <ul class="nav nav-collapse">
                                    <li>
                                        <a href="/admin/music">
                                            <span class="sub-item">Danh sách bài hát</span>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="/admin/category">
                                            <span class="sub-item">Danh sách chuyên mục</span>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="/admin/top">
                                            <span class="sub-item">Danh sách top bài hát</span>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </li>
                    </ul>

                </div>
            </div>
        </div>
        <!-- End Sidebar -->

        <!-- Main panel -->
        <div class="main-panel">
            <div class="content">

                <div class="page-inner">

                    <!-- Flash message -->
                    <div class="form-group">
                        <div id="flash-alert" class="alert alert-danger alert-dismissible fade show"
                            style="display: none;">
                        </div>
                    </div>


                    <div class="page-header">
                        <h4 class="page-title"><i class="fas fa-solid fa-file-invoice"></i> Danh sách tài khoản người
                            dùng</h4>
                    </div>

                    <div class="col-md-12">
                        <div class="card">

                            <div class="card-header">
                                <div class="d-flex align-items-center">
                                    <h4 class="card-title">Thêm người dùng mới</h4>
                                    <button class="btn btn-primary btn-round ml-auto" data-toggle="modal"
                                        data-target="#addRowModal">
                                        <i class="fa fa-plus"></i>
                                        Thêm mới
                                    </button>
                                </div>
                            </div>

                            <div class="card-body">

                                <!-- Modal -->
                                <div class="modal fade" id="addRowModal" tabindex="-1" role="dialog" aria-hidden="true">
                                    <div class="modal-dialog" role="document">
                                        <div class="modal-content">
                                            <div class="modal-header no-bd">
                                                <h2 class="modal-title">
                                                    <span class="fw-mediumbold">Thêm người dùng mới</span>
                                                </h2>
                                                <button type="button" class="close" data-dismiss="modal"
                                                    aria-label="Close">
                                                    <span aria-hidden="true">&times;</span>
                                                </button>
                                            </div>
                                            <div class="modal-body">
                                                <p class="small">Tạo người dùng mới, vui lòng điền đầy đủ thông tin
                                                </p>
                                                <form>
                                                    <div class="row">
                                                        <div class="col-sm-12">
                                                            <div class="form-group form-group-default">
                                                                <label>Họ và Tên</label>
                                                                <input id="addName" type="text" class="form-control"
                                                                    placeholder="Nhập họ tên">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-12 pr-0">
                                                            <div class="form-group form-group-default">
                                                                <label>Email</label>
                                                                <input id="addEmail" type="text" class="form-control"
                                                                    placeholder="Nhập email">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="form-group form-group-default">
                                                                <label>Mật khẩu</label>
                                                                <input id="addPassword" type="text" class="form-control"
                                                                    placeholder="Nhập mật khẩu">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="form-group form-group-default">
                                                                <label>Xác nhận mật khẩu</label>
                                                                <input id="confirmPassword" type="text"
                                                                    class="form-control"
                                                                    placeholder="Xác thực mật khẩu">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </form>
                                            </div>
                                            <div class="modal-footer no-bd">
                                                <button type="button" id="addRowButton"
                                                    class="btn btn-primary">Add</button>
                                                <button type="button" class="btn btn-danger"
                                                    data-dismiss="modal">Close</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!-- Modal delete-->
                                <div class="modal fade" id="deleteRowModal" tabindex="-1" role="dialog"
                                    aria-hidden="true">
                                    <div class="modal-dialog" role="document">
                                        <div class="modal-content">
                                            <div class="modal-header no-bd">
                                                <h2 class="modal-title">
                                                    <span class="fw-mediumbold">Xóa người dùng</span>
                                                </h2>
                                                <button type="button" class="close" data-dismiss="modal"
                                                    aria-label="Close">
                                                    <span aria-hidden="true">&times;</span>
                                                </button>
                                            </div>
                                            <div class="modal-body">
                                                <p>Bạn có muốn xóa người dùng <b><span id="confirm_name"></span></b>
                                                    ?</p>
                                            </div>
                                            <div class="modal-footer no-bd">
                                                <button type="button" id="deleteRowButton"
                                                    class="btn btn-primary">Delete</button>
                                                <button type="button" class="btn btn-danger"
                                                    data-dismiss="modal">Close</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!-- Main table -->
                                <div class="table-responsive">
                                    <table id="add-row" class="display table table-striped table-hover">

                                        <thead>
                                            <tr>
                                                <th><i class="fas fa-user"></i> Tên</th>
                                                <th><i class="fas fa-envelope"></i> Email</th>
                                                <th>Role</th>
                                                <th style="width: 5%"><i class="fas fa-wrench"></i> Action</th>
                                            </tr>
                                        </thead>

                                        <tbody>

                                        </tbody>
                                    </table>
                                </div>

                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
        <!-- End panel -->

    </div>

    </div>


    <!-- /////////// Css, js//////////////////////////// -->
    <!--   Core JS Files   -->
    <script src="/admin/js/core/jquery.3.2.1.min.js"></script>
    <script src="/admin/js/core/popper.min.js"></script>
    <script src="/admin/js/core/bootstrap.min.js"></script>

    <!-- jQuery UI -->
    <script src="/admin/js/plugin/jquery-ui-1.12.1.custom/jquery-ui.min.js"></script>
    <script src="/admin/js/plugin/jquery-ui-touch-punch/jquery.ui.touch-punch.min.js"></script>

    <!-- jQuery Scrollbar -->
    <script src="/admin/js/plugin/jquery-scrollbar/jquery.scrollbar.min.js"></script>

    <!-- Chart JS -->
    <script src="/admin/js/plugin/chart.js/chart.min.js"></script>

    <!-- Datatables -->
    <script src="/admin/js/plugin/datatables/datatables.min.js"></script>

    <!-- Bootstrap Notify -->
    <script src="/admin/js/plugin/bootstrap-notify/bootstrap-notify.min.js"></script>


    <!-- Atlantis JS -->
    <script src="/admin/js/atlantis.min.js"></script>

    <!-- Axious -->
    <script src="/axios/axios.min.js"></script>

    <script src="/admin/js/adminQLTaiKhoan.js"></script>
</body>

</html>